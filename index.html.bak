					},200); 
				});

				$('.button3').click(function(){
					$(this).attr('src','img/buttonF3.png');
					temp = i;
					setTimeout(function() {
						$.mobile.changePage("#rs");
						$('.button3').attr('src','img/button3.png');
					},200); 
				});

				$('.button4').click(function(){
					$(this).attr('src','img/buttonF4.png');
					temp = i;
					setTimeout(function() {
						$.mobile.changePage("#evkid");
						$('.button4').attr('src','img/button4.png');
						centerContent();
					},200); 
				});

				$('.button5').click(function(){
					$(this).attr('src','img/buttonF5.png');
					temp = i;
					setTimeout(function() {
						$.mobile.changePage("#mypas");
						$('.button5').attr('src','img/button5.png');
						centerContent();
					},200); 
				});

				$('.logout').click(function(){
					$(this).attr('src','img/logoutF.png');
					$('.logouttext').css('color','red');
					setTimeout(function() {
						$.mobile.changePage("#login");
						$('.logout').attr('src','img/logout.png');
						$('.logouttext').css('color','black');
					},200); 
				});
				
				$('.grest').click(function(){
					$(this).attr('src','img/grest2.png');
					main = $('#main');
					setTimeout(function() {
						$.mobile.changePage("#main");
						$('.grest').attr('src','img/grest.png');
					},200); 
				});
					
			}
			
			<!-- Center Script -->
		
					function centerContent()
					{
						var container = $('#evkid');
						var content = $('.kid');
						content.css("left", (container.width()-content.width())/2);
						content.css("top", (container.height()-content.height())/2);
						
						var container2 = $('#mypas');
						var content2 = $('.pas');
						content2.css("left", (container2.width()-content2.width())/2);
						content2.css("top", (container2.height()-content2.height())/2);
					}				
				
			$(document).ready(main);
			
		</script>
		  
